{
  "name": "webpack-starter-2",
  "version": "0.0.1",
  "description": "A webpack starter forked from https://github.com/wbkd/webpack-starter / Configured for personal Canvas Studies.",
  "scripts": {
    "lint": "npm run lint:styles; npm run lint:scripts",
